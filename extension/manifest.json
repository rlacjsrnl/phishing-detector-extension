{
  "manifest_version": 3,
  "name": "Phishing Detector",
  "version": "1.0",
  "description": "현재 접속한 사이트가 피싱인지 확인합니다.",
  "permissions": ["tabs"],
  "host_permissions": ["http://127.0.0.1:5000/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}
